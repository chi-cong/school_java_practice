package Student_Console;

public class Student {
	private String id;
	private String name;
	private String classID;
	private String gender;
	private String address;
	private Double mark;
	
	public Student () {
		super();
	}
	public Student (
		String id,
		String name,
		String classID,
		String gender,
		String address,
		Double mark
	) {
		super();
		this.id = id;
		this.name = name;
		this.classID = classID;
		this.gender = gender;
		this.address = address;
		this.mark = mark;
	}
}
